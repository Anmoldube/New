public class Solution {
    public static int findXOR(int L, int R){
                int ans = fuc(L-1) ^ fuc(R);
                return ans;
       }
       public static int  fuc(int n){
            if(n%4 == 1) return 1;
            else if(n%4 == 2) return n+1;
            else if(n%4 ==3) return 0;
            else return n;
     }
}
