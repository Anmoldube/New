import java.util.*;

public class Solution {
    public static List<Integer> sortedArray(int[] a, int[] b) {
         HashSet <Integer> s=new HashSet<>();
  ArrayList < Integer > l1=new ArrayList<>();
  for (int i = 0; i < a.length; i++)
    s.add(a[i]);
  for (int i = 0; i < b.length; i++)
    s.add(b[i]);
  for (int it: s){
    l1.add(it);
  }
  return l1;
 }
}
