class Solution {
  
    public int XOR(int n, int m)
    { 
        // Code here
        return n^m;
    }
    public int check(int a, int b) {
        // Code here
        
        return (b >> (a-1)) & 1;
    }
    public int setBit(int c, int d) {
        // Code here
        d = d | (1 << c);
        return d;
    }
}
