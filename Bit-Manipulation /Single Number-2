class Solution {
    public int singleNumber(int[] nums) {
        int result=0;
        for(int i=0; i<nums.length; i++) {
            result = result^nums[i];
        }
        return result;
    }
}

one more  approach 

class Solution {
    public int singleNumber(int[] nums) {
        // Arrays.sort(nums);
        int once = 0;
        int twos = 0;
        for(int i = 0 ; i < nums.length ; i++){
            once  = (once ^ nums[i]) & ~twos;
            twos  = (twos ^ nums[i]) & ~once;
        }
        return once;
    }
}
