import java.util.Arrays; 
public class Solution {
    public static int[] getSecondOrderElements(int n, int []arr) {
        // Write your code here.
        Arrays.sort(arr);
        int[] result = new int[2];
        result[0] = arr[arr.length - 2];     
        result[1] = arr[1];
        return result;
    }
}
